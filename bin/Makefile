	$(MAKE) -C .. sanitize

sanitize-thread:
	$(MAKE) -C .. sanitize-thread

cli:
	$(MAKE) -C .. cli

lib:
	$(MAKE) -C .. lib

reload-shaders:
	$(MAKE) -C .. reload-shaders

clean:
	$(MAKE) -C .. clean

distclean:
	$(MAKE) -C .. distclean
